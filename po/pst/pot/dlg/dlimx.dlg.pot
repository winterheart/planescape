msgid "Pet the Lim-Lim."
msgstr ""

#: 21873 21876 21881
msgid "Kick the Lim-Lim."
msgstr ""

#: 21874 21877 21882
msgid "Leave the Lim-Lim alone."
msgstr ""

#: 21875
msgid "Pet the Lim-Lim some more."
msgstr ""

#: 21878
msgid "Let it go."
msgstr ""

#: 21879
msgid "Chase after it, try and kick it again."
msgstr ""
